# Android path
# Refer https://developer.android.com/studio/command-line
export ANDROID_HOME=${HOME}/Library/Android/sdk
export ANDROID_PLATFORM=${ANDROID_HOME}/platform-tools
export PATH="$PATH:${ANDROID_PLATFORM}"
alias emulator=${ANDROID_HOME}/emulator/emulator

# flutter
export PATH="$HOME/flutter/bin:$PATH"

# tmuxp binaries
export PATH="$PATH:$HOME/Library/Python/3.9/bin"

# go
export GOPATH=$HOME/go
export GOROOT=/usr/local/opt/go/libexec
export PATH=$PATH:$GOPATH/bin:$GOROOT/bin

# cargo
export PATH="$PATH:$HOME/.cargo/bin"

# gcloud
export PATH="$PATH:$(brew --prefix)/Caskroom/google-cloud-sdk/latest/google-cloud-sdk/bin"

# enable krew
export PATH="${PATH}:${HOME}/.krew/bin"

export PATH="/usr/local/sbin:$PATH"
export GPG_TTY=$(tty)
#export EDITOR=nvim

# jenv
export PATH="$HOME/.jenv/bin:$PATH"
eval "$(jenv init -)"
export JAVA_HOME="$(jenv prefix)"

# docker-machine
# eval $(docker-machine env default)

#sqlite
export PATH="/usr/local/opt/sqlite/bin:$PATH"
